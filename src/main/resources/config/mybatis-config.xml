<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd" >
<configuration>
	
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<!-- 별칭 등록 -->
	<!-- 유의사항 : 미리 등록해두려면 해당 클래스가 당장 존재해야 됨 -->
	<typeAliases>
		<typeAlias type="com.ppicachu.ppic.member.model.vo.Member" alias="Member" />
		<typeAlias type="com.ppicachu.ppic.mail.model.vo.Mail" alias="Mail"/>
		<typeAlias type="com.ppicachu.ppic.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.ppicachu.ppic.chat.model.vo.Chat" alias="Chat"/>
		<typeAlias type="com.ppicachu.ppic.comschedule.model.vo.Comschedule" alias="Comschedule"/>
		<typeAlias type="com.ppicachu.ppic.notice.model.vo.Notice" alias="Notice"/>
		<typeAlias type="com.ppicachu.ppic.notification.model.vo.Notification" alias="Notification"/>
		<typeAlias type="com.ppicachu.ppic.common.model.vo.Attachment" alias="Attachment"/>
		<typeAlias type="com.ppicachu.ppic.work.model.vo.Work" alias="Work" />
		<typeAlias type="com.ppicachu.ppic.board.model.vo.Report" alias="Report" />
		<typeAlias type="com.ppicachu.ppic.approval.model.vo.Approval" alias="Approval" />
		<typeAlias type="com.ppicachu.ppic.document.model.vo.Document" alias="Document" />
		<typeAlias type="com.ppicachu.ppic.member.model.vo.Department" alias="Department" />
	</typeAliases>

	
	<!-- mapper 등록 -->
	<!-- 유의사항 : 미리 등록해두려면 해당 mapper파일이 현재 존재해야 됨 + namespace도 지정되어있어야 됨 -->
	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/mail-mapper.xml" />
		<mapper resource="/mappers/board-mapper.xml" />
		<mapper resource="/mappers/chat-mapper.xml" />
		<mapper resource="/mappers/comschedule-mapper.xml" />
		<mapper resource="/mappers/notice-mapper.xml" />
		<mapper resource="/mappers/notification-mapper.xml" />
		<mapper resource="/mappers/work-mapper.xml" />
		<mapper resource="/mappers/approval-mapper.xml" />
		<mapper resource="/mappers/document-mapper.xml" />
	</mappers>
	
	
</configuration>
